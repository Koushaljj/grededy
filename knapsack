
    import java.util.*;

class Item {
    int weight;
    int value;

    Item(int weight, int value) {
        this.weight = weight;
        this.value = value;
    }
}

public class fk {

    public static double fk(int W, Item[] items) {

        
        Arrays.sort(items, (a, b) -> 
            Double.compare((double)b.value / b.weight, (double)a.value / a.weight)
        );

        double maxValue = 0.0;

        
        for (Item item : items) {
            if (W == 0) break;

            if (item.weight <= W) {
                
                maxValue += item.value;
                W -= item.weight;
            } else {
                
                double fraction = (double) W / item.weight;
                maxValue += item.value * fraction;
                W = 0; 
            }
        }

        return maxValue;
    }

    public static void main(String[] args) {
        Item[] items = {
            new Item(10, 60),
            new Item(20, 100),
            new Item(30, 120)
        };

        int capacity = 50;

        System.out.println("Maximum value in Knapsack = " + fk(capacity, items));
    }
}


