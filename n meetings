import java.util.Arrays;

class Meeting {
    int start;
    int end;
    Meeting(int s, int e) { start = s; end = e; }
}

public class Solution {
    
    public static int maxMeetings(Meeting[] meetings) {
        if (meetings == null || meetings.length == 0) return 0;

        Arrays.sort(meetings, (a, b) -> Integer.compare(a.end, b.end));

        int count = 0;
        int lastEnd = -1; 

        for (Meeting m : meetings) {
            if (m.start >= lastEnd) {
                count++;
                lastEnd = m.end;
            }
        }

        return count;
    }

    
    public static void main(String[] args) {
        Meeting[] meetings = {
            new Meeting(1, 2),
            new Meeting(3, 4),
            new Meeting(0, 6),
            new Meeting(8, 9),
            new Meeting(5, 7),
            new Meeting(5, 9)
        };

        System.out.println("Maximum meetings: " + maxMeetings(meetings)); // prints 4
    }
}
